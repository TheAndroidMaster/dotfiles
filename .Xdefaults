URxvt*foreground: white
URxvt*background: black

URxvt*font: xft:Roboto Mono:size=8:antialias=true

! gray/white
*color0:  #404040
*color8:  #808080
*color7:  #F5F5F5
*color15: #FFFFFF
! red
*color1:  #FF5252
*color9:  #FF8A80
! green
*color2:  #20B684
*color10: #67EFB8
! orange
*color3:  #FFDE03
*color11: #FFFF5B
! blue
*color4:  #0088FF
*color12: #70BCFF
! magenta
*color5:  #D831B0
*color13: #FF71E8
! cyan
*color6:  #008899
*color14: #33BBFF

URxvt.scrollBar: false

URxvt.perl-ext-common:		...,autocomplete-ALL-the-things
URxvt.keysym.Control-Tab:	perl:aAtt:word-complete
URxvt.keysym.Control-question:	perl:aAtt:fuzzy-complete
URxvt.keysym.Control-quotedbl:	perl:aAtt:undo
